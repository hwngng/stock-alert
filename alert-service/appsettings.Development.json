{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ThreadSize": 5,
  "WebSocket": {
    "DataSourceApi": "ws://localhost:9876/realtime/?EIO=4&transport=websocket",
